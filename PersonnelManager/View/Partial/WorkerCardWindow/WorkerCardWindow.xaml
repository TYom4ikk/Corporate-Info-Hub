<Window x:Class="PersonnelManager.View.Partial.WorkerCardWindow.WorkerCardWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:WorkerCard="clr-namespace:PersonnelManager.View.Partial.WorkerCardWindow"
        mc:Ignorable="d"
        Title="WorkerCardWindow" Height="450" Width="800">
    <Grid Background="DarkGray">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <StackPanel Grid.Column="0" Background="White" Margin="16">
            <WorkerCard:WorkerCardFillData x:Name="WorkerCardFillData"/>
        </StackPanel>

        <StackPanel Grid.Column="1" Margin="16">
            <WorkerCard:WorkerCardFillCalendars x:Name="WorkerCardFillCalendars"/>
        </StackPanel>

    </Grid>
</Window>
